{
  "name": "cahnfc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build:dev": "cross-env NODE_ENV=production next build",
    "build:staging": "cross-env NODE_ENV=production next build",
    "build:production": "cross-env NODE_ENV=production next build",
    "type-check": "tsc",
    "lint": "next lint",
    "lint:check": "eslint \"src/**/*\" ./pages/** -c .eslintrc.json --ext .ts,.tsx",
    "lint:fix": "eslint \"src/**/*\" ./pages/** -c .eslintrc.json --fix --ext .ts,.tsx",
    "format:check": "prettier --check \"{,!(.next)/**/}*.{ts, tsx}\"",
    "format:fix": "prettier --write \"{,!(.next)/**/}*.{ts, tsx}\"",
    "prepare": "husky install",
    "postinstall": "husky install",
    "test-all": "npm run format:check && npm run lint:check && npm run type-check"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn format:fix",
      "yarn lint:fix",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.10.0",
    "@emotion/react": "^11.10.0",
